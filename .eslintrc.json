{
    "extends": "airbnb",

    "ecmaFeatures": {
        "jsx": true,
        "modules": true
      },
      "env": {
        "browser": true,
        "node": true
      },
      "parser": "babel-eslint",
      "rules": {
        "quotes": [2, "single"],
        "strict": [2, "never"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-underscore-dangle": 0
      },
      "plugins": [
        "react",
        "jsx-a11y",
        "import"
      ]
}
